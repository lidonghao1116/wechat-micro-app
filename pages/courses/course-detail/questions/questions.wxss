.detail-head {
  position: relative;
  height: 88rpx;
  background: #ffffff;
  line-height: 88rpx;
  text-align: center;
  border-bottom: 1px solid #ff7f3a;
}
.detail-head .detail-nav {
  position: absolute;
  left: 0;
  top: 0;
  padding: 30rpx;
  width: 15rpx;
  height: 28rpx;
  display: inline-block;
}
.detail-head .detail-title {
  font-size: 34rpx;
  color: #333;
}
.detail-head .detail-title .logo {
  width: 36rpx;
  height: 36rpx;
}
.detail-head .share-redPacket {
  position: absolute;
  right: 30rpx;
  top: 0;
}
.detail-head .share-redPacket image {
  width: 47rpx;
  height: 40rpx;
  vertical-align: middle;
}
.detail-head .share-redPacket text {
  display: inline-block;
  margin-left: 10rpx;
  font-size: 20rpx;
  width: 40rpx;
  line-height: 20rpx;
  vertical-align: middle;
}
.detail-head .moreCert {
  position: absolute;
  right: 30rpx;
  top: 0;
  display: block;
  height: 100%;
  line-height: 88rpx;
  color: #ff7f3a;
}
page {
  background: #ffffff;
}
.tip-container .tips {
  width: 100%;
  height: 180rpx;
  text-align: center;
  margin: 100rpx auto 60rpx;
}
.tip-container .tips image {
  width: 120rpx;
  height: 180rpx;
}
.tip-container .tipTxt {
  color: #333333;
  font-size: 34rpx;
  text-align: center;
  line-height: 54rpx;
}
.tip-container .tipSm {
  font-size: 30rpx;
  color: #808080;
  text-align: center;
  margin: 70rpx auto 90rpx;
}
.tip-container .btn {
  display: block;
  height: 88rpx;
  margin: 30rpx;
  text-align: center;
  line-height: 88rpx;
  border-radius: 8rpx;
  font-size: 36rpx;
}
.tip-container .beginBtn {
  background: #f0854b;
  color: #ffffff;
}
.tip-container .returnBack {
  background: #fbfafc;
  color: #333;
  border: 1rpx solid #d6d5d6;
  margin-bottom: 0;
}
.scoresBtn {
  margin-top: 80rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  text-align: center;
  overflow: hidden;
  margin-left: -4%;
}
.scoresBtn .answerTips {
  height: 40rpx;
  margin-bottom: 60rpx;
  font-size: 32rpx;
}
.scoresBtn .col-50 {
  width: 46%;
  margin-left: 4%;
  float: left;
}
.scoresBtn .col-50 .button {
  display: inline-block;
  height: 72rpx;
  line-height: 72rpx;
  padding: 0 40rpx;
  color: #f0854b;
  font-size: 28rpx;
}
.wrong {
  color: red;
}
.correct {
  color: green;
}
.white_btn {
  border: 1rpx solid #f0854b;
  color: #f0854b !important;
  background-color: #fff!important;
}
.button-fill {
  color: #fff!important;
  background: #f0854b !important;
}
.button_box {
  margin-top: 40rpx;
  padding: 0 60rpx;
}
.button_box .button-big {
  font-size: 34rpx;
  height: 96rpx;
  line-height: 96rpx;
}
.button_box .button {
  width: 100%;
  background: #f0854b;
  border-radius: 8rpx;
  text-align: center;
}
.content-block-title {
  margin: 30rpx 15rpx 10rpx;
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 28rpx;
  text-transform: uppercase;
  line-height: 1.5;
  color: #6d6d72;
  white-space: initial;
}
.list-block {
  margin: 10rpx 0 35rpx;
  font-size: 36rpx;
}
.list-block .answer {
  background: #fff;
  padding: 0;
  margin: 0;
  position: relative;
  border-top: 1px solid #e7e7e7;
  border-bottom: 1px solid #e7e7e7;
}
.list-block .answer .item-content {
  padding-left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.list-block .answer .item-content:not(:last-child) .item-inner {
  border-bottom: 1rpx solid #e7e7e7;
}
.list-block .answer .item-media {
  padding-top: 18rpx;
  padding-bottom: 20rpx;
}
.list-block .answer .item-inner {
  display: block;
  padding-top: 26rpx;
  align-self: stretch;
  margin-left: 15rpx;
  width: 100%;
}
.list-block .answer .item-inner .item-text {
  font-size: 30rpx;
  color: #5f646e;
  overflow: hidden;
  text-overflow: ellipsis;
}
.no-border {
  border-bottom: none!important;
}
#qsBox .time {
  text-align: center;
  margin-top: 60rpx;
  font-size: 34rpx;
}
