.detail-head {
  position: relative;
  height: 88rpx;
  background: #ffffff;
  line-height: 88rpx;
  text-align: center;
  border-bottom: 1px solid #ff7f3a;
}
.detail-head .detail-nav {
  position: absolute;
  left: 0;
  top: 0;
  padding: 30rpx;
  width: 15rpx;
  height: 28rpx;
  display: inline-block;
}
.detail-head .detail-title {
  font-size: 34rpx;
  color: #333;
}
.detail-head .detail-title .logo {
  width: 36rpx;
  height: 36rpx;
}
.detail-head .share-redPacket {
  position: absolute;
  right: 30rpx;
  top: 0;
}
.detail-head .share-redPacket image {
  width: 47rpx;
  height: 40rpx;
  vertical-align: middle;
}
.detail-head .share-redPacket text {
  display: inline-block;
  margin-left: 10rpx;
  font-size: 20rpx;
  width: 40rpx;
  line-height: 20rpx;
  vertical-align: middle;
}
.detail-head .moreCert {
  position: absolute;
  right: 30rpx;
  top: 0;
  display: block;
  height: 100%;
  line-height: 88rpx;
  color: #ff7f3a;
}
.scoresBtn {
  margin-top: 80rpx;
  padding-left: 40rpx;
  padding-right: 40rpx;
  text-align: center;
  overflow: hidden;
  margin-left: -4%;
}
.scoresBtn .answerTips {
  height: 40rpx;
  margin-bottom: 60rpx;
  font-size: 32rpx;
}
.scoresBtn .col-50 {
  width: 46%;
  margin-left: 4%;
  float: left;
}
.scoresBtn .col-50 .button {
  display: inline-block;
  height: 72rpx;
  line-height: 72rpx;
  padding: 0 40rpx;
  color: #f0854b;
  font-size: 28rpx;
}
.wrong {
  color: red;
}
.correct {
  color: green;
}
.white_btn {
  border: 1rpx solid #f0854b;
  color: #f0854b !important;
  background-color: #fff!important;
}
.button-fill {
  color: #fff!important;
  background: #f0854b !important;
}
.button_box {
  margin-top: 40rpx;
  padding: 0 60rpx;
}
.button_box .button-big {
  font-size: 34rpx;
  height: 96rpx;
  line-height: 96rpx;
}
.button_box .button {
  width: 100%;
  background: #f0854b;
  border-radius: 8rpx;
  text-align: center;
}
page {
  background: #ffffff;
}
.score {
  margin-top: 64rpx;
}
.score .center {
  text-align: center;
}
.score .center image {
  width: 120rpx;
  height: 120rpx;
}
.score .center .scores-title {
  color: #f0854b;
  font-size: 32rpx;
  margin-top: 16rpx;
  text-align: center;
}
.score .center .hx {
  font-size: 40rpx;
  margin-top: 80rpx;
}
.score .scores-lists {
  margin-top: 40rpx;
  font-size: 32rpx;
}
.score .scores-lists .row {
  margin-bottom: 32rpx;
  overflow: hidden;
  margin-left: -4%;
}
.score .scores-lists .row .col-40 {
  width: 36%;
  margin-left: 4%;
  float: left;
}
.score .scores-lists .row .col-60 {
  width: 56%;
  margin-left: 4%;
  float: left;
}
.right {
  text-align: right;
}
.left {
  text-align: left;
}
