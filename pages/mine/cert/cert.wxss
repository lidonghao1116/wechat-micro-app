.detail-head {
  position: relative;
  height: 88rpx;
  background: #ffffff;
  line-height: 88rpx;
  text-align: center;
  border-bottom: 1px solid #ff7f3a;
}
.detail-head .detail-nav {
  position: absolute;
  left: 0;
  top: 0;
  padding: 30rpx;
  width: 15rpx;
  height: 28rpx;
  display: inline-block;
}
.detail-head .detail-title {
  font-size: 34rpx;
  color: #333;
}
.detail-head .detail-title .logo {
  width: 36rpx;
  height: 36rpx;
}
.detail-head .share-redPacket {
  position: absolute;
  right: 30rpx;
  top: 0;
}
.detail-head .share-redPacket image {
  width: 47rpx;
  height: 40rpx;
  vertical-align: middle;
}
.detail-head .share-redPacket text {
  display: inline-block;
  margin-left: 10rpx;
  font-size: 20rpx;
  width: 40rpx;
  line-height: 20rpx;
  vertical-align: middle;
}
.detail-head .moreCert {
  position: absolute;
  right: 30rpx;
  top: 0;
  display: block;
  height: 100%;
  line-height: 88rpx;
  color: #ff7f3a;
}
.no_info {
  font-size: 24rpx;
  color: #808080;
  margin-top: 60rpx;
}
.nothingBox {
  height: 100%;
  text-align: center;
}
.nothingBox image {
  margin-top: 240rpx;
  width: 233rpx;
  height: 189rpx;
}
.certificate {
  background-color: #fff;
  margin-top: 24rpx;
  padding: 24rpx 0 24rpx 40rpx;
}
.certificate:last-child {
  margin-bottom: 40rpx;
}
.certificate .certificate_tt {
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 40rpx;
  color: #ff7f3a;
  font-size: 32rpx;
  font-weight: bold;
  position: relative;
}
.certificate .certificate_tt:before {
  content: '';
  position: absolute;
  width: 18rpx;
  height: 18rpx;
  background: #ff7f3a;
  left: 0;
  top: 24rpx;
}
.certificate .result_t {
  width: 100%;
  margin-top: 26rpx;
}
.certificate .result_t .certlist {
  margin-bottom: 30rpx;
}
.certificate .result_t .certlist view {
  display: inline-block;
  vertical-align: middle;
}
.certificate .result_t .certlist .certValue {
  width: 300rpx;
  /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/
  color: #3d3d3d;
  word-break: keep-all;
  /* 不换行 */
  white-space: nowrap;
  /* 不换行 */
  overflow: hidden;
  /* 内容超出宽度时隐藏超出部分的内容 */
  text-overflow: ellipsis;
}
.certificate .result_t .certlist .grey {
  width: 140rpx;
  color: #808080;
}
.certificate .result_t .certlist .certLValue {
  width: 300rpx;
  /* 当对象内文本溢出时显示省略标记(...) ；需与overflow:hidden;一起使用。*/
  color: #3d3d3d;
  word-break: keep-all;
  /* 不换行 */
  white-space: nowrap;
  /* 不换行 */
  overflow: hidden;
  /* 内容超出宽度时隐藏超出部分的内容 */
  text-overflow: ellipsis;
  width: 500rpx;
}
